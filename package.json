{
  "name": "upis-client",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">= 16.13.0",
    "yarn": "1.22.10"
  },
  "scripts": {
    "start": "next start",
    "start:dev": "next dev",
    "build": "next build",
    "compile": "tsc",
    "browserslist": "npx browserslist",
    "update": "npx npm-check-updates",
    "lint": "yarn lint:code && yarn lint:prettier && yarn lint:styles && yarn lint:filenames && yarn compile",
    "lint:code": "npx eslint src --ext .ts,.tsx",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:styles": "npx stylelint \"**/*.scss\"",
    "lint:styles:fix": "yarn lint:styles --fix",
    "lint:filenames": "npx @ls-lint/ls-lint",
    "lint:prettier": "npx prettier --check \"src/**\"",
    "lint:prettier:fix": "npx prettier --write \"src/**\""
  },
  "dependencies": {
    "@axe-core/react": "^4.3.2",
    "clsx": "^1.1.1",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.6.0",
    "next": "^12.0.9",
    "next-auth": "^4.2.1",
    "next-redux-wrapper": "^6.0.2",
    "next-translate": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.5.3",
    "react-hook-form": "^7.25.3",
    "react-leaflet": "^3.2.5",
    "react-query": "^3.34.12",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5"
