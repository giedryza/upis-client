{
  "name": "upis-client",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">= 16.13.0",
    "yarn": "1.22.10"
  },
  "scripts": {
    "start": "next start",
    "start:dev": "next dev",
    "build": "next build",
    "compile": "tsc",
    "browserslist": "npx browserslist",
    "update": "npx npm-check-updates",
    "lint": "yarn lint:code && yarn lint:code-style && yarn lint:styles && yarn lint:filenames && yarn compile",
    "lint:code": "npx eslint src --ext .ts,.tsx",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:styles": "npx stylelint \"**/*.scss\"",
    "lint:styles:fix": "yarn lint:styles --fix",
    "lint:filenames": "npx @ls-lint/ls-lint",
    "lint:code-style": "npx prettier --check \"src/**\"",
    "lint:code-style:fix": "npx prettier --write \"src/**\""
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.6.0",
    "next": "11",
    "next-redux-wrapper": "^6.0.2",
    "next-translate": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.5.3",
    "react-hook-form": "^7.25.3",
    "react-leaflet": "^3.2.5",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5"
  },
  "devDependencies": {
    "@axe-core/react": "^4.3.2",
    "@ls-lint/ls-lint": "^1.10.0",
    "@svgr/webpack": "^5.5.0",
    "@types/leaflet": "^1.7.9",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "typescript": "^4.4.2"
  }
}
